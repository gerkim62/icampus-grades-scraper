{
    "manifest_version": 3,
    "name": "Scraper Extension",
    "version": "1.0",
    "description": "A simple scraper extension",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_icon": {
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["content.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  